digraph "WeatherRecord"
{
 // INTERACTIVE_SVG=YES
 // LATEX_PDF_SIZE
  edge [fontname="Helvetica",fontsize="10",labelfontname="Helvetica",labelfontsize="10"];
  node [fontname="Helvetica",fontsize="10",shape=record];
  Node1 [label="{WeatherRecord\n|+ m_windSpeed\l+ m_temperature\l+ m_solarRadiation\l|}",height=0.2,width=0.4,color="black", fillcolor="grey75", style="filled", fontcolor="black",tooltip="Structure to hold a single weather record entry."];
  Node2 -> Node1 [color="grey25",fontsize="10",style="solid",label=" +m_time" ,arrowhead="odiamond",fontname="Helvetica"];
  Node2 [label="{Time\n||+ Time()\l+ Time()\l+ SetHour()\l+ GetHour()\l+ SetMinute()\l+ GetMinute()\l+ SetSecond()\l+ GetSecond()\l}",height=0.2,width=0.4,color="black", fillcolor="white", style="filled",URL="$class_time.html",tooltip="Represents a time of day with hour, minute, and second."];
  Node3 -> Node1 [color="grey25",fontsize="10",style="solid",label=" +m_date" ,arrowhead="odiamond",fontname="Helvetica"];
  Node3 [label="{Date\n||+ Date()\l+ Date()\l+ SetDay()\l+ GetDay()\l+ SetMonth()\l+ GetMonth()\l+ SetYear()\l+ GetYear()\l}",height=0.2,width=0.4,color="black", fillcolor="white", style="filled",URL="$class_date.html",tooltip="Declaration of the Date class."];
}
